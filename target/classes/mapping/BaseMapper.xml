<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="edu.zhku.jsj144.lzc.video.mapper.BaseMapper">
	<insert id="create">
		insert into video
		values(#{vid}, #{uid}, #{title}, #{category},
		#{description}, #{pid}, #{permission},
		#{password}, #{path}, #{verify}, #{datetime})
	</insert>
</mapper>